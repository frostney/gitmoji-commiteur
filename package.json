{
  "name": "gitmoji-commiteur",
  "version": "1.1.0",
  "description": "Use emojis for your commits! :zap: :sparkles:",
  "main": "index.js",
  "bin": {
    "gitmoji-commiteur": "./bin/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier-eslint --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "commit": "node ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/gitmoji-commiteur.git"
  },
  "keywords": [
    "emoji",
    "gitmoji",
    "commit"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frostney/gitmoji-commiteur/issues"
  },
  "homepage": "https://github.com/frostney/gitmoji-commiteur#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "node-emoji": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.1",
    "commitlint-config-gitmoji": "^1.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier-eslint-cli": "^4.7.1"
  }
}
